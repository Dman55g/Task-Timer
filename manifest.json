{
    "manifest_version": 3,
    "name": "Task:Timer",
    "description": "A task timer that allows for easy task management inside of chrome.",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": ["notifications"],
    "host_permissions": [],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
